#include <iostream>

using namespace std;


int fibonacci(int n) {
if(n<=1)
return n;
else{
    return fibonacci(n-1)+fibonacci(n-2);
}


    // Complete the function.
    // The function is already completed so what is the purpose...
    // do I just make a pr...

}

int main() {
    int n;
    cin >> n;
    cout << fibonacci(n);
    return 0;
}
